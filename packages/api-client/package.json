{
  "name": "@rocket-fist/api-client",
  "version": "0.0.1",
  "private": true,
  "description": "API client for Rocket Fist backend",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "import": "./src/*.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@rocket-fist/domain": "*"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  }
}

